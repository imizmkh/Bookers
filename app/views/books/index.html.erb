<h1>Books</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>
        <%= @books.each do |book| %>
        <span><%= list.title %></span>
        <% end %>
      </td>
      <td>
        <%= @books.each do |book| %>
        <span><%= list.body %></span>
        <% end %>
      </td>
    </tr>
  </tbody>
</table>
